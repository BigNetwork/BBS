<% if :cart_id.nil? %>
   <p>
      <%= f.label :cart_id %><br />
      <%= f.collection_select :cart_id, Cart.all, :id, :id %>
    </p>
<% else %>
  <%= f.hidden_field :cart_id %>
<% end %>

<p>
  <%= f.label :product_type_id %><br />
  <%= f.collection_select :product_type_id, ProductType.all, :id, :name %>
</p>
<p>
  <%= f.label :quantity %><br />
  <%= f.text_field :quantity %>
</p>